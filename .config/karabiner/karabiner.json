{
    "global": {
        "check_for_updates_on_startup": true,
        "show_in_menu_bar": false,
        "show_profile_name_in_menu_bar": false
    },
    "profiles": [
        {
            "complex_modifications": {
                "parameters": {
                    "basic.simultaneous_threshold_milliseconds": 50,
                    "basic.to_delayed_action_delay_milliseconds": 200,
                    "basic.to_if_alone_timeout_milliseconds": 600,
                    "basic.to_if_held_down_threshold_milliseconds": 200,
                    "mouse_motion_to_scroll.speed": 100
                },
                "rules": [
                    {
                        "description": "Post escape if caps lock is pressed alone.",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": {
                                        "optional": [
                                            "any"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "left_command"
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "escape"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Mute or unmute Output/Input while pressing F11/F12",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "f12"
                                },
                                "to": [
                                    {
                                        "shell_command": "osascript -e 'set volume input volume 0'"
                                    }
                                ],
                                "to_after_key_up": [
                                    {
                                        "shell_command": "osascript -e 'set volume input volume 100'"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "f11"
                                },
                                "to": [
                                    {
                                        "shell_command": "osascript -e 'set volume output muted true'"
                                    }
                                ],
                                "to_after_key_up": [
                                    {
                                        "shell_command": "osascript -e 'set volume output muted false'"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Parentheses, Curly braces and Square brackets on shift keys",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "left_shift"
                                },
                                "to": [
                                    {
                                        "key_code": "left_shift"
                                    }
                                ],
                                "to_if_alone": [
                                    {
                                        "key_code": "9",
                                        "modifiers": [
                                            "left_shift"
                                        ]
                                    },
                                    {
                                        "key_code": "0",
                                        "modifiers": [
                                            "left_shift"
                                        ]
                                    },
                                    {
                                        "key_code": "left_arrow"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Vim Cursor with left-alt, semicolon and quote are Home and End",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "k",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_alt"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "up_arrow"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "j",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_alt"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "down_arrow"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "h",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_alt"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "left_arrow"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "l",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_alt"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "right_arrow"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "quote",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_alt"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "end"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "semicolon",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_alt"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "home"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Fast Sleep",
                        "manipulators": [
                            {
                                "type": "basic",
                                "from": {
                                    "key_code": "print_screen"
                                },
                                "to": [
                                    {"shell_command": "osascript -e 'tell application \"System Events\" to sleep'"}
                                ]
                            }
                        ]
                    },
                    {
                        "description": "Reload Spotify",
                        "manipulators": [
                            {
                                "type": "basic",
                                "from": {
                                    "key_code": "f5"
                                },
                                "to": [
                                    {"shell_command": "osascript -e 'quit app \"Spotify\"' -e 'delay 2'  -e 'launch application  \"Spotify\"' -e 'tell application \"Spotify\" to play'"}
                                ]
                            }
                        ]
                    },
                    {
                        "description": "Fast Close",
                        "manipulators": [
                            {
                                "type": "basic",
                                "from": {
                                    "key_code": "pause"
                                },
                                "to": [
                                    {"shell_command": "osascript -e 'quit app \"Whatsapp\"'"}
                                ]
                            }
                        ]
                    },
                    {
                        "description": "Open Terminal on Ctrl+`",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": [
                                            "^com\\.microsoft\\.VSCode"
                                        ],
                                        "type": "frontmost_application_unless"
                                    }
                                ],
                                "from": {
                                    "key_code": "grave_accent_and_tilde",
                                    "modifiers": {
                                        "mandatory": [
                                            "right_control"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "osascript -e 'tell app \"iTerm2\"' -e 'create window with default profile' -e activate -e end"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
